{"ast":null,"code":"import axios from 'axios';\nexport default {\n  name: 'DeleteVisitanteComponent',\n  data() {\n    return {\n      cpfExcluir: ''\n    };\n  },\n  methods: {\n    async excluirVisitantePorCpf() {\n      try {\n        const response = await axios.delete(`http://localhost:8081/visitantes/${this.cpfExcluir}`);\n        if (response.status === 204) {\n          alert('Visitante excluído com sucesso!');\n          this.cpfExcluir = ''; // Limpar o campo após a exclusão\n        } else {\n          alert('404 (Visitante não encontrado)');\n        }\n      } catch (error) {\n        if (error.response && error.response.status === 404) {\n          alert('404 (Visitante não encontrado)');\n        } else {\n          console.error('Erro ao excluir visitante:', error);\n          alert('Falha ao excluir visitante.');\n        }\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","name","data","cpfExcluir","methods","excluirVisitantePorCpf","response","delete","status","alert","error","console"],"sources":["C:\\Users\\vgmen\\OneDrive\\Documentos\\Faculdade\\5 SEMESTRE\\web-1\\sigcondominio-front-main\\sigcondominio-front-main\\visitante-app\\src\\components\\DeleteVisitanteComponent.vue"],"sourcesContent":["<template>\n  <div>\n    <h2>Excluir dados de visitante</h2>\n\n    <div>\n      <h3>Apagar dados</h3>\n\n    <!-- <div @submit.prevent=\"excluirVisitantePorCpf\" class=\"row g-3 align-items-center\">\n      <div class=\"col-auto\">q\n        <label for=\"cpfExcluir\" class=\"col-form-label\">Excluir por CPF: </label>\n      </div>\n      <div class=\"col-auto\">\n        <input type=\"text\" id=\"cpfExcluir\" class=\"form-control\" v-model=\"cpfExcluir\" required/>\n      </div>\n      <button type=\"submit\">Excluir</button>\n    </div> -->\n      <form @submit.prevent=\"excluirVisitantePorCpf\">\n        <div class=\"row g-3 align-items-center\">\n          <div class=\"col-auto\">\n            <label for=\"cpfExcluir\" class=\"col-form-label\">Excluir por CPF: </label>\n          </div>\n          <div class=\"col-auto\">\n            <input type=\"text\" id=\"cpfExcluir\" class=\"form-control\" v-model=\"cpfExcluir\" required />\n          </div>\n          <div class=\"col-auto\">\n            <button type=\"submit\" class=\"btn btn-alert\">Excluir</button>\n          </div>\n        </div>\n      </form>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from 'axios';\n\nexport default {\n  name: 'DeleteVisitanteComponent',\n  data() {\n    return {\n      cpfExcluir: ''\n    }\n  },\n  methods: {\n    async excluirVisitantePorCpf() {\n      try {\n        const response = await axios.delete(`http://localhost:8081/visitantes/${this.cpfExcluir}`);\n        if(response.status === 204) {\n          alert('Visitante excluído com sucesso!');\n          this.cpfExcluir = '';  // Limpar o campo após a exclusão\n        } else {\n          alert('404 (Visitante não encontrado)');\n        }\n      } catch (error) {\n        if (error.response && error.response.status === 404) {\n          alert('404 (Visitante não encontrado)');\n        } else {\n          console.error('Erro ao excluir visitante:', error);\n          alert('Falha ao excluir visitante.');\n        }\n      }\n    }\n  }\n};\n</script>\n"],"mappings":"AAkCA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAI,EAAE,0BAA0B;EAChCC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,UAAU,EAAE;IACd;EACF,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,sBAAsBA,CAAA,EAAG;MAC7B,IAAI;QACF,MAAMC,QAAO,GAAI,MAAMN,KAAK,CAACO,MAAM,CAAC,oCAAoC,IAAI,CAACJ,UAAU,EAAE,CAAC;QAC1F,IAAGG,QAAQ,CAACE,MAAK,KAAM,GAAG,EAAE;UAC1BC,KAAK,CAAC,iCAAiC,CAAC;UACxC,IAAI,CAACN,UAAS,GAAI,EAAE,EAAG;QACzB,OAAO;UACLM,KAAK,CAAC,gCAAgC,CAAC;QACzC;MACF,EAAE,OAAOC,KAAK,EAAE;QACd,IAAIA,KAAK,CAACJ,QAAO,IAAKI,KAAK,CAACJ,QAAQ,CAACE,MAAK,KAAM,GAAG,EAAE;UACnDC,KAAK,CAAC,gCAAgC,CAAC;QACzC,OAAO;UACLE,OAAO,CAACD,KAAK,CAAC,4BAA4B,EAAEA,KAAK,CAAC;UAClDD,KAAK,CAAC,6BAA6B,CAAC;QACtC;MACF;IACF;EACF;AACF,CAAC","ignoreList":[]},"me