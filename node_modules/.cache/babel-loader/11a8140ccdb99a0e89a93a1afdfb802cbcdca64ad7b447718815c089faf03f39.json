{"ast":null,"code":"import axios from 'axios';\nexport default {\n  name: 'BuscarVisitanteComponent',\n  data() {\n    return {\n      cpfBuscar: '',\n      visitante: null\n    };\n  },\n  methods: {\n    async buscarVisitantePorCpf() {\n      try {\n        const response = await axios.get(`http://localhost:8081/visitantes/cpf/${this.cpfBuscar}`);\n        if (response.status === 200) {\n          this.visitante = response.data;\n          alert('Visitante encontrado.');\n        } else {\n          alert('404 (Visitante não encontrado)');\n          this.visitante = null;\n        }\n      } catch (error) {\n        console.error('Erro ao buscar visitante: ', error);\n        alert('Falha ao buscar visitante.');\n        this.visitante = null;\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","name","data","cpfBuscar","visitante","methods","buscarVisitantePorCpf","response","get","status","alert","error","console"],"sources":["C:\\Users\\vgmen\\OneDrive\\Documentos\\Faculdade\\5 SEMESTRE\\web-1\\sigcondominio-front-main\\sigcondominio-front-main\\visitante-app\\src\\components\\BuscarVisitanteComponent.vue"],"sourcesContent":["<template>\n  <h1>Buscar visitante por CPF</h1>\n\n  <nav class=\"navbar navbar-light bg-light justify-content-between\">\n    <a class=\"navbar-brand\">Buscar visitante por CPF</a>\n    <form @submit.prevent=\"buscarVisitantePorCpf\" class=\"form-inline d-flex\">\n    <input class=\"form-control mr-sm-2\" type=\"search\" id=\"cpfBuscar\" v-model=\"cpfBuscar\" required placeholder=\"Insira aqui o CPF\" aria-label=\"Search\"/>\n    <button class=\"btn btn-outline-success my-2 my-sm-0\" type=\"submit\">Pesquisar</button>\n    </form>\n  </nav>\n\n  <div v-if=\"visitante\">\n    <br>\n    <h2>Dados: </h2>\n    <p>Nome: {{ visitante.nome }}</p>\n    <p>CPF: {{ visitante.cpf }}</p>\n    <br>\n  </div>\n</template>\n\n<script>\nimport axios from 'axios';\n\nexport default {\n  name: 'BuscarVisitanteComponent',\n  data() {\n    return {\n      cpfBuscar: '',\n      visitante: null,\n    }\n  },\n\n  methods: {\n    async buscarVisitantePorCpf() {\n      try {\n        const response = await axios.get(`http://localhost:8081/visitantes/cpf/${this.cpfBuscar}`);\n\n        if (response.status === 200) {\n          this.visitante = response.data;\n          alert('Visitante encontrado.');\n        } else {\n          alert('404 (Visitante não encontrado)');\n          this.visitante = null;\n        }\n      } catch (error) {\n        console.error('Erro ao buscar visitante: ', error);\n        alert('Falha ao buscar visitante.');\n        this.visitante = null;\n      }\n    }\n  }\n};\n</script>\n\n<style>\n  .form-inline .form-control {\n    width: auto;\n    flex: 1;\n  }\n</style>"],"mappings":"AAqBA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAI,EAAE,0BAA0B;EAChCC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,SAAS,EAAE,EAAE;MACbC,SAAS,EAAE;IACb;EACF,CAAC;EAEDC,OAAO,EAAE;IACP,MAAMC,qBAAqBA,CAAA,EAAG;MAC5B,IAAI;QACF,MAAMC,QAAO,GAAI,MAAMP,KAAK,CAACQ,GAAG,CAAC,wCAAwC,IAAI,CAACL,SAAS,EAAE,CAAC;QAE1F,IAAII,QAAQ,CAACE,MAAK,KAAM,GAAG,EAAE;UAC3B,IAAI,CAACL,SAAQ,GAAIG,QAAQ,CAACL,IAAI;UAC9BQ,KAAK,CAAC,uBAAuB,CAAC;QAChC,OAAO;UACLA,KAAK,CAAC,gCAAgC,CAAC;UACvC,IAAI,CAACN,SAAQ,GAAI,IAAI;QACvB;MACF,EAAE,OAAOO,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,4BAA4B,EAAEA,KAAK,CAAC;QAClDD,KAAK,CAAC,4BAA4B,CAAC;QACnC,IAAI,CAACN,SAAQ,GAAI,IAAI;MACvB;IACF;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}