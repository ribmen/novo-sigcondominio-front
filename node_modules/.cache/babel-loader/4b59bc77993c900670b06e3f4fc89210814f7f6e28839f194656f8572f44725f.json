{"ast":null,"code":"import axios from 'axios';\nexport default {\n  name: 'AtualizarVisitanteComponent',\n  data() {\n    return {\n      visitanteId: '',\n      visitante: null\n    };\n  },\n  methods: {\n    async buscarVisitantePorId() {\n      try {\n        const response = await axios.get(`http://localhost:8081/visitantes/${this.visitanteId}`);\n        if (response.status === 200) {\n          this.visitante = response.data;\n        } else {\n          alert('Visitante não encontrado');\n          this.visitante = null;\n        }\n      } catch (error) {\n        console.error('Erro ao buscar visitante: ', error);\n        alert('Erro ao buscar visitante');\n        this.visitante = null;\n      }\n    },\n    async atualizarVisitante() {\n      try {\n        const response = await axios.put(`http://localhost:8081/visitantes/${this.visitanteId}`, this.visitante);\n        if (response.status === 200) {\n          alert('Visitante atualizado com sucesso');\n        } else {\n          alert('Erro ao atualizar visitante');\n        }\n      } catch (error) {\n        console.error('Erro ao atualizar visitante: ', error);\n        alert('Erro ao atualizar visitante');\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","name","data","visitanteId","visitante","methods","buscarVisitantePorId","response","get","status","alert","error","console","atualizarVisitante","put"],"sources":["C:\\Users\\vgmen\\OneDrive\\Documentos\\Faculdade\\5 SEMESTRE\\web-1\\sigcondominio-front-main\\sigcondominio-front-main\\visitante-app\\src\\components\\AtualizarVisitanteComponent.vue"],"sourcesContent":["<template>\n  <div>\n    <h3>Atualizar Visitante</h3>\n\n    <form @submit.prevent=\"buscarVisitantePorId\">\n        <div class=\"row g-3 align-items-center\">\n          <div class=\"col-auto\">\n            <label for=\"visitanteId\" class=\"col-form-label\">ID do visitante: </label>\n          </div>\n          <div class=\"col-auto\">\n            <input type=\"text\" id=\"visitanteId\" class=\"form-control\" v-model=\"visitanteId\" required />\n          </div>\n          <div class=\"col-auto\">\n            <button type=\"submit\" class=\"btn btn-secondary\">Buscar</button>\n          </div>\n        </div>\n      </form><br>\n\n    <div v-if=\"visitante\">\n      <h5>Atualizar Dados</h5>\n\n      <form @submit.prevent=\"atualizarVisitante\">\n        <div class=\"row g-3 align-items-center\">\n          <div class=\"col-auto\">\n            <label for=\"nome\" class=\"col-form-label\">Nome: </label>\n          </div>\n          <div class=\"col-auto\">\n            <input type=\"text\" id=\"nome\" class=\"form-control\" v-model=\"visitante.nome\" required />\n          </div>\n          <div class=\"col-auto\">\n            <label for=\"cpf\" class=\"col-form-label\">CPF: </label>\n          </div>\n          <div class=\"col-auto\">\n            <input type=\"text\" id=\"cpf\" class=\"form-control\" v-model=\"visitante.cpf\" required />\n          </div>\n\n          <div>\n            <button type=\"submit\" class=\"btn btn-secondary\">Atualizar</button>\n          </div>\n        </div>\n      </form>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from 'axios';\n\nexport default {\n  name: 'AtualizarVisitanteComponent',\n  data() {\n    return {\n      visitanteId: '',\n      visitante: null,\n    };\n  },\n  methods: {\n    async buscarVisitantePorId() {\n      try {\n        const response = await axios.get(`http://localhost:8081/visitantes/${this.visitanteId}`);\n        if (response.status === 200) {\n          this.visitante = response.data;\n        } else {\n          alert('Visitante não encontrado');\n          this.visitante = null;\n        }\n      } catch (error) {\n        console.error('Erro ao buscar visitante: ', error);\n        alert('Erro ao buscar visitante');\n        this.visitante = null;\n      }\n    },\n    async atualizarVisitante() {\n      try {\n        const response = await axios.put(`http://localhost:8081/visitantes/${this.visitanteId}`, this.visitante);\n        if (response.status === 200) {\n          alert('Visitante atualizado com sucesso');\n        } else {\n          alert('Erro ao atualizar visitante');\n        }\n      } catch (error) {\n        console.error('Erro ao atualizar visitante: ', error);\n        alert('Erro ao atualizar visitante');\n      }\n    },\n  },\n};\n</script>\n"],"mappings":"AA8CA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAI,EAAE,6BAA6B;EACnCC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,WAAW,EAAE,EAAE;MACfC,SAAS,EAAE;IACb,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,oBAAoBA,CAAA,EAAG;MAC3B,IAAI;QACF,MAAMC,QAAO,GAAI,MAAMP,KAAK,CAACQ,GAAG,CAAC,oCAAoC,IAAI,CAACL,WAAW,EAAE,CAAC;QACxF,IAAII,QAAQ,CAACE,MAAK,KAAM,GAAG,EAAE;UAC3B,IAAI,CAACL,SAAQ,GAAIG,QAAQ,CAACL,IAAI;QAChC,OAAO;UACLQ,KAAK,CAAC,0BAA0B,CAAC;UACjC,IAAI,CAACN,SAAQ,GAAI,IAAI;QACvB;MACF,EAAE,OAAOO,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,4BAA4B,EAAEA,KAAK,CAAC;QAClDD,KAAK,CAAC,0BAA0B,CAAC;QACjC,IAAI,CAACN,SAAQ,GAAI,IAAI;MACvB;IACF,CAAC;IACD,MAAMS,kBAAkBA,CAAA,EAAG;MACzB,IAAI;QACF,MAAMN,QAAO,GAAI,MAAMP,KAAK,CAACc,GAAG,CAAC,oCAAoC,IAAI,CAACX,WAAW,EAAE,EAAE,IAAI,CAACC,SAAS,CAAC;QACxG,IAAIG,QAAQ,CAACE,MAAK,KAAM,GAAG,EAAE;UAC3BC,KAAK,CAAC,kCAAkC,CAAC;QAC3C,OAAO;UACLA,KAAK,CAAC,6BAA6B,CAAC;QACtC;MACF,EAAE,OAAOC,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,+BAA+B,EAAEA,KAAK,CAAC;QACrDD,KAAK,CAAC,6BAA6B,CAAC;MACtC;IACF;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}